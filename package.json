  "homepage": "https://github.com/SilentImp/emailParser#readme",
  "dependencies": {
    "amqplib": "^0.5.6",
    "dotenv": "^8.2.0",
    "fastify": "^2.15.1",
    "fastify-graceful-shutdown": "^2.0.1",
    "fastify-healthcheck": "^2.1.0",
